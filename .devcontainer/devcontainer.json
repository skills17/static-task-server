{
  "name": "skills17-static-task-server",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "workspaceFolder": "/workspaces/static-task-server",
  "remoteUser": "node",
  "extensions": [
    "dbaeumer.vscode-eslint",
    "redhat.vscode-yaml"
  ],
  "settings": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.tabSize": 2,
    "editor.rulers": [100]
  },
  "postCreateCommand": "npm install"
}
